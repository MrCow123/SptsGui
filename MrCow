coroutine.wrap(BECZNMZ_fake_script)()
local function PNLP_fake_script() -- Credits.LocalScript 
	local script = Instance.new('LocalScript', Credits)

	script.Parent.MouseButton1Down:connect(function()
		script.Parent.Parent.AuraTab.Visible = false
		script.Parent.Parent.RespawnTab.Visible = false
		script.Parent.Parent.MiscTab.Visible = false
		script.Parent.Parent.CreditsTab.Visible = true
	end)
end
coroutine.wrap(PNLP_fake_script)()
local function QFIDQIV_fake_script() -- MainFrame.LocalScript 
	local script = Instance.new('LocalScript', MainFrame)

	local UIS = game:GetService('UserInputService')
	local frame = script.Parent
	local dragToggle = nil
	local dragSpeed = 0.25
	local dragStart = nil
	local startPos = nil
	
	local function updateInput(input)
		local delta = input.Position - dragStart
		local position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X,
			startPos.Y.Scale, startPos.Y.Offset + delta.Y)
		game:GetService('TweenService'):Create(frame, TweenInfo.new(dragSpeed), {Position = position}):Play()
	end
	
	frame.InputBegan:Connect(function(input)
		if (input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch) then 
			dragToggle = true
			dragStart = input.Position
			startPos = frame.Position
			input.Changed:Connect(function()
				if input.UserInputState == Enum.UserInputState.End then
					dragToggle = false
				end
			end)
		end
	end)
	
	UIS.InputChanged:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
			if dragToggle then
				updateInput(input)
			end
		end
	end)
end
coroutine.wrap(QFIDQIV_fake_script)()

PurpleLightning.MouseButton1Click:Connect(function()
	game:GetService("Workspace").thecowisamazing123.LeftHand.Aura2:destroy()
	game:GetService("Workspace").thecowisamazing123.LeftHand.Aura3:destroy()
	game:GetService("Workspace").thecowisamazing123.LeftHand.Aura1:destroy()
	game:GetService("Workspace").thecowisamazing123.RightHand.Aura1:destroy()
	game:GetService("Workspace").thecowisamazing123.RightHand.Aura2:destroy()
	game:GetService("Workspace").thecowisamazing123.RightHand.Aura3:destroy()
	game:GetService("Workspace").thecowisamazing123.RightUpperLeg.Aura1:destroy()
	game:GetService("Workspace").thecowisamazing123.LeftUpperLeg.Aura1:destroy()
	game:GetService("Workspace").thecowisamazing123.UpperTorso.Aura1:destroy()
end)

