-- Scripts:

local function SYNVIMI_fake_script() -- Aura.LocalScript 
	local script = Instance.new('LocalScript', Aura)

	script.Parent.MouseButton1Down:connect(function()
		script.Parent.Parent.AuraTab.Visible = true
		script.Parent.Parent.RespawnTab.Visible = false
		script.Parent.Parent.MiscTab.Visible = false
		script.Parent.Parent.CreditsTab.Visible = false
	end)
end
coroutine.wrap(SYNVIMI_fake_script)()
local function ROTUG_fake_script() -- Respawn.LocalScript 
	local script = Instance.new('LocalScript', Respawn)

	script.Parent.MouseButton1Down:connect(function()
		script.Parent.Parent.AuraTab.Visible = false
		script.Parent.Parent.RespawnTab.Visible = true
		script.Parent.Parent.MiscTab.Visible = false
		script.Parent.Parent.CreditsTab.Visible = false
	end)
end
coroutine.wrap(ROTUG_fake_script)()
